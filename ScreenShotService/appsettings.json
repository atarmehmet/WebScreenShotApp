{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "ShotterThreadCount": 1,
  "RetrieverThreadCount": 1,
  "InputFilesFolder": "C:/ScreenShot/InputUrlFiles/",
  "OutputFilesFolder": "C:/ScreenShot/OutputUrlFiles/",
  "OutputImagesFolder": "C:/ScreenShot/OutputImagesFolder/",
  "ProcessedFilesFolder": "C:/ScreenShot/ProcessedFiles/",
  "InputFilePattern": "Input*.txt",
  "OutputFilePattern": "Output*.txt",
  "WindowSize": "1920x1600",
  "MongoDB": "mongodb://127.0.0.1:27017;ScreenShot;WebScreen"
}
